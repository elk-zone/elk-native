{
  "build": {
    "beforeBuildCommand": "cd elk && npm run build",
    "beforeDevCommand": "cd elk && npm run dev",
    "devPath": "http://localhost:5314",
    "distDir": "../elk/.output/public"
  },
  "package": {
    "productName": "Elk"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "open": true
      },
      "window": {
        "startDragging": true,
        "maximize": true,
        "unmaximize": true
      }
    },
    "bundle": {
      "active": true,
      "category": "SocialNetworking",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons-dev/32x32.png",
        "icons-dev/128x128.png",
        "icons-dev/128x128@2x.png",
        "icons-dev/icon.icns",
        "icons-dev/icon.ico"
      ],
      "identifier": "zone.elk.native",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": {
        "default-src": "'none'",
        "style-src": "'self' 'unsafe-inline'",
        "img-src": "'self' data: https:",
        "media-src": "https:",
        "font-src": "'self'",
        "connect-src": "https: wss:"
      }
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://raw.githubusercontent.com/elk-zone/elk-native/main/latest-nightly.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVBOTdGNjcwNDJDODNERDEKUldUUlBjaENjUGFYV2xySGcwcEtMUmRBRjhWUC82QWxTcGVqUmZpc1RQOG8zeTJWL0V0NEY4VEMK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "Elk",
        "width": 1000,
        "minWidth": 640,
        "minHeight": 300,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      }
    ],
    "systemTray": {
      "title": "Elk",
      "iconPath": "icons-dev/icon.png"
    }
  }
}
